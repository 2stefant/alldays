{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "object-literal-shorthand": false,
    "object-literal-sort-keys": false,
    "no-duplicate-variable": true,
    "max-line-length": {
      "options": [
        120
      ]
    },
    "ordered-imports": false,
    "no-consecutive-blank-lines": false
  },
  "jsRules": {
    "object-literal-sort-keys": false,
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}